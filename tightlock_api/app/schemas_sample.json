{
  "title": "Available Schemas",
  "$ref": "#/definitions/Schemas",
  "definitions": {
    "LocalFile": {
      "title": "LocalFile",
      "type": "object",
      "properties": {
        "type": {
          "title": "Type",
          "default": "local_file",
          "enum": ["local_file"],
          "type": "string"
        },
        "location": {
          "title": "Location",
          "type": "string"
        }
      },
      "required": ["location"]
    },
    "BigQuery": {
      "title": "BigQuery",
      "type": "object",
      "properties": {
        "project": {
          "title": "Project",
          "type": "string"
        },
        "dataset": {
          "title": "Dataset",
          "type": "string"
        },
        "table": {
          "title": "Table",
          "type": "string"
        },
        "credentials": {
          "title": "Credentials",
          "type": "string"
        }
      },
      "required": ["project", "dataset", "table"]
    },
    "GA4Web": {
      "title": "GA4Web",
      "type": "object",
      "properties": {
        "type": {
          "title": "Type",
          "default": "GA4MP",
          "enum": ["GA4MP"],
          "type": "string"
        },
        "api_secret": {
          "title": "Api Secret",
          "type": "string"
        },
        "non_personalized_ads": {
          "title": "Non Personalized Ads",
          "default": false,
          "type": "boolean"
        },
        "debug": {
          "title": "Debug",
          "default": false,
          "type": "boolean"
        },
        "user_properties": {
          "title": "User Properties",
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          }
        },
        "event_type": {
          "title": "Event Type",
          "enum": ["gtag"],
          "type": "string"
        },
        "measurement_id": {
          "title": "Measurement Id",
          "type": "string"
        }
      },
      "required": ["api_secret", "event_type", "measurement_id"]
    },
    "GA4App": {
      "title": "GA4App",
      "type": "object",
      "properties": {
        "type": {
          "title": "Type",
          "default": "GA4MP",
          "enum": ["GA4MP"],
          "type": "string"
        },
        "api_secret": {
          "title": "Api Secret",
          "type": "string"
        },
        "non_personalized_ads": {
          "title": "Non Personalized Ads",
          "default": false,
          "type": "boolean"
        },
        "debug": {
          "title": "Debug",
          "default": false,
          "type": "boolean"
        },
        "user_properties": {
          "title": "User Properties",
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          }
        },
        "event_type": {
          "title": "Event Type",
          "enum": ["firebase"],
          "type": "string"
        },
        "firebase_app_id": {
          "title": "Firebase App Id",
          "type": "string"
        }
      },
      "required": ["api_secret", "event_type", "firebase_app_id"]
    },
    "GADSMarketing": {
      "title": "GADSMarketing",
      "type": "object",
      "properties": {
        "type": {
          "title": "Type",
          "default": "GADS_MARKETING",
          "enum": ["GADS_MARKETING"],
          "type": "string"
        },
        "client_id": {
          "title": "Client ID",
          "type": "string"
        },
        "client_secret": {
          "title": "Client Secret",
          "type": "string"
        },
        "developer_token": {
          "title": "Developer Token",
          "type": "string"
        },
        "login_customer_id": {
          "title": "Login Customer ID",
          "type": "string"
        },
        "refresh_token": {
          "title": "Refresh Token",
          "type": "string"
        },
        "user_list_id": {
          "title": "User List ID",
          "type": "string"
        },
        "audience_name": {
          "title": "Audience Name",
          "type": "string"
        },
        "payload_type": {
          "title": "Payload Type",
          "enum": ["CREATE_AUDIENCE", "UPDATE_AUDIENCE", "DELETE_AUDIENCE"],
          "type": "string"
        }
      },
      "required": [
        "client_id",
        "client_secret",
        "developer_token",
        "login_customer_id",
        "refresh_token",
        "user_list_id",
        "audience_name",
        "payload_type"
      ]
    },
    "MetaMarketing": {
      "title": "MetaMarketing",
      "type": "object",
      "properties": {
        "type": {
          "title": "Type",
          "default": "META_MARKETING",
          "enum": ["META_MARKETING"],
          "type": "string"
        },
        "access_token": {
          "title": "Access Token",
          "type": "string"
        },
        "ad_account_id": {
          "title": "Ad Account ID",
          "type": "string"
        },
        "audience_name": {
          "title": "Audience Name",
          "type": "string"
        },
        "payload_type": {
          "title": "Payload Type",
          "enum": ["CREATE_USER", "UPDATE_USER"],
          "type": "string"
        },
        "customer_file_source": {
          "title": "Customer File Source",
          "default": "USER_PROVIDED_ONLY",
          "type": "string"
        },
        "schema": {
          "title": "Schema",
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": ["EMAIL", "PHONE", "FN"]
        }
      },
      "required": [
        "access_token",
        "ad_account_id",
        "audience_name",
        "payload_type"
      ]
    },
    "Schemas": {
      "title": "Schemas",
      "type": "object",
      "properties": {
        "sources": {
          "title": "Sources",
          "type": "array",
          "items": {
            "anyOf": [
              {
                "$ref": "#/definitions/LocalFile"
              },
              {
                "$ref": "#/definitions/BigQuery"
              }
            ]
          }
        },
        "destinations": {
          "title": "Destinations",
          "type": "array",
          "items": {
            "anyOf": [
              {
                "oneOf": [
                  {
                    "$ref": "#/definitions/GA4Web"
                  },
                  {
                    "$ref": "#/definitions/GA4App"
                  }
                ]
              },
              {
                "$ref": "#/definitions/GADSMarketing"
              },
              {
                "$ref": "#/definitions/MetaMarketing"
              }
            ]
          }
        }
      },
      "required": ["sources", "destinations"]
    }
  }
}